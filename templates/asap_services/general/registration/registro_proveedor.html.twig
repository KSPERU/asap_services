{% extends 'base.html.twig' %}

{% block title %}
  Registro proveedor
{% endblock %}

{% block stylesheets %}
  {{ encore_entry_link_tags('app') }}
  <link rel="stylesheet" href="{{ asset('css/registration/registro_proveedor.css') }}" />
  <link rel="stylesheet" href="{{ asset('css/proveedor/terminos_condiciones_prov.css') }}" />
{% endblock %}

{% block javascripts %}
  {{ encore_entry_script_tags('app') }}
  <script>
    function previewImage(event, querySelector){
      const input = event.target;
      $imgPreview = document.querySelector(querySelector);
      if(!input.files.length) return
      file = input.files[0];
      objectURL = URL.createObjectURL(file);
      $imgPreview.src = objectURL;
    }

    function verCV() {
      const inputCV = document.getElementById('registration_prov_form_idPersona_p_cv').files[0].name;
      document.getElementById('listadocv').innerHTML = inputCV;
    }

    function verAP() {
      const inputAP = document.getElementById('registration_prov_form_idPersona_p_antpen').files[0].name;
      document.getElementById('listadoap').innerHTML = inputAP;
    }

    function verAC() {
      const inputAC = document.getElementById('registration_prov_form_idPersona_p_cert').files[0].name;
      document.getElementById('listadoac').innerHTML = inputAC;
    }
  </script>
{% endblock %}

